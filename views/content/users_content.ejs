<!-- users -->

<div class="container">
  <div class="row">
<% for(i=0; i<pageData.length; i++) { %>
    <div class="col-md-3 p-4">
      <div class="card" style="font-size: 14px;">
        <img class="card-img-top" src="<%= pageData[i].photo %>" alt="Card image cap">
        <div class="card-body">
          <p class="card-title text-center"><strong><%= pageData[i].first_name %> <%= pageData[i].last_name %></strong></p>
          <hr>
          <a href="/user/<%= i %>" class="float-right">View Profile</a>
        </div>
      </div>
    </div>
<% }; %>
  </div>
</div>

<br><br>